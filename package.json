{
	"name": "tinybrain",
	"version": "0.0.1",
	"main": "index.js",
	"author": "skoshx",
	"license": "MIT",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "tsup src/exports.ts --format esm,cjs && mv dist/exports.js dist/exports.mjs",
		"test": "node test/tensor.js",
		"prettier-check": "prettier --check .",
		"format": "prettier --write .",
		"type-check": "tsc --noEmit",
		"release": "np --preview"
	},
	"prettier": {
		"useTabs": true,
		"printWidth": 100,
		"singleQuote": true,
		"trailingComma": "none"
	},
	"keywords": [
		"typescript",
		"tensor",
		"autograd"
	],
	"devDependencies": {
		"@preconstruct/cli": "^2.2.2",
		"prettier": "^2.7.1",
		"tsup": "^6.2.3",
		"typescript": "^4.8.2"
	},
	"dependencies": {
		"tiny-invariant": "^1.3.1"
	}
}
